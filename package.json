{
  "name": "trading-app-platform",
  "version": "0.1.0",
  "description": "trading app platform with nextjs, pothos, prisma, graphql, typescript, tailwindcss",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "clean:generated": "rm -rf generated prisma/generated",
    "type-check": "tsc --noEmit",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@apollo/client": "^4.0.2",
    "@hookform/resolvers": "^5.2.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@pothos/core": "^4.2.0",
    "@pothos/plugin-prisma": "^4.2.1",
    "@pothos/plugin-relay": "^4.6.2",
    "@pothos/plugin-scope-auth": "^4.1.5",
    "@pothos/plugin-validation": "^3.10.2",
    "@prisma/client": "^5.18.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@rupeezy/jsvortex": "^2.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "critters": "^0.0.25",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.24.2",
    "graphql-yoga": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.536.0",
    "next": "14.2.5",
    "next-auth": "^5.0.0-beta.21",
    "prisma": "^5.18.0",
    "prisma-generator-pothos-codegen": "^0.7.1",
    "protobufjs": "^7.5.4",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "rxjs": "^7.8.2",
    "stocksocket": "^2.1.2",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.6",
    "uuid": "^11.1.0",
    "ws": "^8.18.3",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/ws": "^8.18.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "keywords": [
    "nextjs",
    "graphql",
    "prisma",
    "pothos",
    "autocrud",
    "typescript"
  ]
}
